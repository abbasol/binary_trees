	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch binary_tree_size",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/11-size",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build binary_tree_size"
		},
		{
			"name": "(gdb) Launch binary_tree_is_full",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/15-full",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "build binary_tree_is_full"
		},
	]
}
